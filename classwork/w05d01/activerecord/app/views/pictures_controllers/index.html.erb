<p id="notice"><%= notice %></p>

<h1>Pictures Controllers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Artist</th>
      <th>String</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pictures_controllers.each do |pictures_controller| %>
      <tr>
        <td><%= pictures_controller.name %></td>
        <td><%= pictures_controller.description %></td>
        <td><%= pictures_controller.artist %></td>
        <td><%= pictures_controller.string %></td>
        <td><%= image_tag pictures_controller.url %></td>
        <td><%= link_to 'Show', pictures_controller %></td>
        <td><%= link_to 'Edit', edit_pictures_controller_path(pictures_controller) %></td>
        <td><%= link_to 'Destroy', pictures_controller, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pictures Controller', new_pictures_controller_path %>
